@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix cc: <http://creativecommons.org/ns#> .
@prefix od: <http://opendiscovery.org/rdf/Model#> .
@prefix tc: <http://opendiscovery.org/rdf/Concept/> .
@prefix ex: <http://example.orf/EvolutionExample/Boats/> .

<http://opendiscovery.org/rdg/Ontologies/FunctionalAnalysis/>
    cc:attributionName "The Open Discovery Project" ;
    cc:attributionURL <http://opendiscovery.org> ;
    cc:license <http://creativecommons.org/publicdomain/zero/1.0/> ;
    a owl:Ontology ;
    rdfs:label "Functional Analysis - Example Washing Machine" ;
    owl:imports <http://opendiscovery.org/rdf/Ontologies/FunctionalAnalysis/> .

# Components

ex:Washingmachine
    a tc:System ;
    skos:prefLabel "Washingmachine"@en, "Waschmaschine"@de .

ex:Electormotor
    a tc:Component ;
    skos:prefLabel "Electro Motor"@en, "Elektromotor"@de .

ex:Washingmachinedrum
    a tc:Component ;
    skos:prefLabel "Washing Machine Drum"@en, "Waschtrommel"@de .

ex:Washingmachinedoor
    a tc:Component ;
    skos:prefLabel "Washing Machine Door"@en, "Waschmaschinentür"@de .

ex:Lock
    a tc:Component ;
    skos:prefLabel "Lock"@en, "Schloss"@de .

ex:Laundry
    a tc:Component ;
    skos:prefLabel "Laundry"@en, "Wäsche"@de .

ex:Waterentry
    a tc:Component ;
    skos:prefLabel "Water Entry"@en, "Wassereinfuhr"@de .



# Actions

ex:Turn
    a tc:Action ;
    skos:prefLabel "Turn"@en, "Drehen"@de .

ex:Open 
    a tc:Action ;
    skos:prefLabel "Open"@en, "Öffnen"@de .

ex:Close 
    a tc:Action ;
    skos:prefLabel "Close"@en, "Verschließen"@de .



# Waschtrommel - Umwälzen - Wäsche

ex:WashingmachinedrumLaundry
    a tc:Interaction ;
    skos:prefLabel "Washing Machine Drum - Laundry"@en, "Waschtrommel - Wäsche"@de .

ex:WashingmachinedrumTurnLaundry
    a tc:SubjectActionObject ;
    tc:Action ex:Turn ;
    tc:Interaction ex:WashingmachinedrumLaundry ;
    skos:prefLabel "Washing Machine Drum - Turn - Laundry"@en, "Waschtrommel - Drehen - Wäsche"@de .

ex:CirculateLaundry
    a tc:Function ;
    tc:SubjectActionObject ex:WashingmachinedrumTurnLaundry ;
    tc:QualityOfFunction tc:UsefulFunction ;
    skos:prefLabel "Ciruclate Laundry"@en, "Umwälzen der Wäsche"@de .



# Wassereinfuhr - Öffnet - Wäsche

ex:WaterentryLaundry
    a tc:Interaction ;
    skos:prefLabel "Water Entry - Laundry"@en, "Wassereinfuhr - Wäsche"@de .

ex:WaterentryOpenLaundry
    a skos:SubjectActionObject ;
    tc:Action ex:Open ;
    tc:Interaction ex:WaterentryLaundry ;
    skos:prefLabel "Water Entry - Open - Laundry"@en, "Wassereinfuhr - Öffnet - Laundry"@de .

ex:MoistureLaundry 
    a tc:Function ;
    tc:SubjectActionObject ex:WaterentryOpenLaundry ;
    tc:QualityOfFunction tc:UsefulFunction ;
    skos:prefLabel "Moisture Laundry"@en, "Befeuchten der Wäsche"@de .



# Elektromotor - bewegt - Waschtrommel

ex:ElectromotorWashingmachinedrum
    a tc:Interaction ;
    skos:prefLabel "Electro Motor - Washing Machine Drum"@en, "Elektromotor - Waschtrommel"@de .

ex:ElectromotorTurnWashingmachinedrum
    a tc:Interaction ;
    tc:Action ex:Turn ;
    tc:Interaction ex:ElectromotorWashingmachinedrum ;
    skos:prefLabel "Electro Motor - Turn - Washing Machine Drum"@en, "Elektromotor - Dreht - Waschtrommel"@de .

ex:MoveWashingmachinedrum
    a tc:Function ;
    tc:SubjectActionObject ex:ElectromotorTurnWashingmachinedrum ;
    tc:QualityOfFunction tc:UsefulFunction ;
    skos:prefLabel "Move Washing Machine Drum"@en, "Bewegen der Waschtrommel"@de .



# Schloss - Bewegen - Waschmaschinentür

ex:LockWashingmachinedoor
    a tc:Interaction ;
    skos:prefLabel "Lock - Washing Machine Door"@en, "Schloss - Waschmachinentür"@de .

ex:LockCloseWashingmachinedoor
    a tc:SubjectActionObject ;
    tc:Action ex:Close ;
    tc:Interaction ex:LockWashingmachinedoor ;
    skos:prefLabel "Lock - Close - Washing Machine Door"@en, "Schloss - Verschließen - Waschmachinentür"@de .

ex:LockDoor
    a tc:Function ;
    tc:SubjectActionObject ex:LockCloseWashingmachinedoor ;
    tc:QualityOfFunction tc:UsefulFunction ;
    skos:prefLabel "Lock Door"@en, "Tür verriegeln"@de .